# Launch polybar
# exec_always --no-startup-id ~/.config/polybar/launch.sh

exec_always fish

# Mod Keys
# $mod is primary modifier for all keybinds
# $sec is secondary modifier for use in specific contexts (see below)
# <Alt>
set $mod Mod4
# <Super>
set $sec Mod1

# Default applications
set $nsid --no-startup-id
set $launcher "rofi -show drun -modi drun,run,ssh"
set $switcher "rofi -show window"
set $term termite
set $browser /opt/vivaldi/vivaldi-bin
set $fileman nemo
set $compositor compton
set $notify dunst

# Desktop layout <tiling|tabbed|stacked>
workspace_layout default


# Font for window titles
# Will also be used by the bar unless a different font is used in the bar {} block below.
font xft:Fira Sans 11

# Border style <normal|1pixel|pixel xx|none|pixel>
new_window pixel 1
new_float normal
hide_edge_borders none

# Workspace names
# to display names or symbols instead of plain workspace numbers you can use
# something like: set $ws1 1:mail
#                 set $ws2 2:
set $ws1 "1:  "
set $ws2 "2: "
set $ws3 "3: "
set $ws4 "4: "
set $ws5 5
set $ws6 6
set $ws7 7
set $ws8 8

# Switch to workspace with urgent window automatically
for_window [urgent=latest] focus

set $coloract "#5c6c7d"
set $colorin "#3f4a56"
set $terminal urxvtc
set_from_resource $color15 i3wm.color15 "#d4d4d5"
set_from_resource $split i3wm.color10 "#998778"
set_from_resource $coloru i3wm.color2 "#998778"
set_from_resource $color0 i3wm.color0 "#131412"
set_from_resource $color8 i3wm.color8 "#949495"

# Theme colors
 # class                 border    backgr    text    indicator child_border
client.focused          $coloract $coloract $color15 $split  $coloract
client.focused_inactive $coloract $coloract $color15 $split  $colorin
client.unfocused        $colorin  $colorin  $color15 $split  $colorin
client.urgent           $coloru   $coloru   $color15 $coloru $coloru
client.placeholder      $color0   $color8   $color15 $color0 $color8
client.background       $color0

