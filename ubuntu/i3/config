#bumblebee
set $transparent #00000000

bar {
	font pango:Inconsolata 10
	position bottom
	tray_output none
	status_command ~/.config/bumblebee-status/bumblebee-status -m brightness disk:root cpu memory battery date time pasink pasource -p root.path=/ time.format="%H:%M CW %V" date.format="%a, %b %d %Y" -t solarized-powerline
	colors {
				background $transparent
                separator #2ECC71
                background #000000
                statusline #DDDDDD
                #<colorclass> <border> <background> <text>
                focused_workspace #586E75 #586E75 #000000
                active_workspace #000000 #A4A28D #FFFFFF
                inactive_workspace #000000 #000000 #DDDDDD
                urgent_workspace #2ECC71 #900000 #FFFFFF
	}
}

set $bg-color            #2f343f
set $inactive-bg-color   #2f343f
set $text-color          #f3f4f5
set $inactive-text-color #676E7D
set $urgent-bg-color #E53935

# Hide/unhide i3status bar
bindsym $mod+z bar mode dock
bindsym $mod+Shift+z bar mode invisible

