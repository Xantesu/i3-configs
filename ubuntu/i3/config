for_window [class="_startup-terminal_"] floating enable, move absolute position 25 25, resize set 680 840
exec $nsid $term --class=_startup-terminal_ -e 'sh -c "cmatrix | lolcat; clear; /bin/zsh"'

exec_always xinput set-prop "ELAN0650:01 04F3:304B Touchpad" "libinput Tapping Enabled" 1

exec_always xinput set-prop "ELAN0650:01 04F3:304B Touchpad" libinput Middle Emulation Enabled" 1

exec_always xinput set-prop "ELAN0650:01 04F3:304B Touchpad" "libinput Natural Scrolling Enabled" 1

exec_always feh --bg-scale ~/Pictures/mountain.jpg
exec_always --no-startup-id compton



client.focused          #e0d8c3 #e0d8c3 #000000 #2e9ef4   #5896c4
client.focused_inactive          #F0EAD6 #F0EAD6 #000000 #2e9ef4   #5896c4
client.unfocused          #F0EAD6 #F0EAD6 #000000 #2e9ef4   #5896c4

